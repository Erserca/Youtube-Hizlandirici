{
    "name": "speedUp!"
,   "version": "1.0"
,"permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]
,   "manifest_version": 2
,   "author": "Erdoğan Serhat ÇALIŞKAN"
,   "description": "Youtube Hızlandırıcısı"
