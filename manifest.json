,   "icons" : {  "18" : "icon_18.png", "64" : "icon_64.png"}
,"content_scripts": [{
		"css": ["styles.css"],
    "js": ["youtube.js"],
  	"matches": ["https://*.youtube.com/*"]
  }]
}
